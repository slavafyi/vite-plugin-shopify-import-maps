{
  "name": "vite-plugin-shopify-import-maps",
  "version": "0.6.1",
  "description": "Enhances Shopify theme development by adding support for import-maps",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slavafyi/vite-plugin-shopify-import-maps.git"
  },
  "bugs": {
    "url": "https://github.com/slavafyi/vite-plugin-shopify-import-maps/issues"
  },
  "homepage": "https://github.com/slavafyi/vite-plugin-shopify-import-maps#readme",
  "author": "Slava M. <hi@slava.fyi>",
  "keywords": [
    "vite",
    "plugin",
    "shopify",
    "import-maps",
    "shopify-import-maps"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint --ext .ts .",
    "prepublish": "tsup",
    "release": "bumpp && pnpm publish"
  },
  "packageManager": "pnpm@9.0.1",
  "tsup": {
    "entry": [
      "./src/index.ts"
    ],
    "format": "esm",
    "dts": true,
    "clean": true
  },
  "types": "./dist/index.d.ts",
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.7.0",
    "@types/node": "^25.0.9",
    "bumpp": "^10.4.0",
    "es-module-lexer": "^2.0.0",
    "eslint": "^9.39.2",
    "eslint-config-love": "^145.0.0",
    "magic-string": "^0.30.21",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.21"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0",
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12"
    }
  }
}
